    "directory": "packages/gatsby-source-wordpress"
  },
  "license": "MIT",
  "author": "Sebastien Fichot <fichot.sebastien@gmail.com>",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "npm:publish": "npm publish --registry https://npm.groundspeak.biz:4873",
    "npm:login": "npm login --registry https://npm.groundspeak.biz:4873",
    "patch": "npm version patch --no-git-tag-version",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "dependencies": {
