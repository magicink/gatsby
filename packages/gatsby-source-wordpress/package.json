{
  "name": "@geocomponents/gatsby-source-wordpress",
  "version": "3.1.62",
  "description": "Gatsby source plugin for building websites using the WordPress CMS as a data source.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "homepage": "https://github.com/magicink/gatsby/tree/master/packages/gatsby-source-wordpress#readme",
  "bugs": {
    "url": "https://github.com/magicink/gatsby/issues"
  },
  "repository": {
    "type": "git",
